{
    "name": "cypress-angular-nestjs-code-coverage-fullstack",
    "version": "1.0.0",
    "description": "A small repository to show how to run cypress full stack code coverage through Angular and Nestjs",
    "main": "index.js",
    "scripts": {
        "start": "concurrently npm:start:client npm:start:server",
        "start:client": "cd angular && npm run start",
        "start:server": "cd nestjs && npm run start:coverage",
        "nyc": "nyc report --report-dir ./coverage --temp-dir /Users/florianpascouau/Workspace/cypress-angular-nestjs-code-coverage-fullstack/.nyc_output --reporter=lcov --reporter=clover --reporter=json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flogh/cypress-angular-nestjs-code-coverage-fullstack.git"
    },
    "keywords": [
        "cypress",
        "angular",
        "nestjs",
        "code",
        "coverage",
        "fullstack"
    ],
    "author": "Florian Pascouau",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/flogh/cypress-angular-nestjs-code-coverage-fullstack/issues"
    },
    "homepage": "https://github.com/flogh/cypress-angular-nestjs-code-coverage-fullstack#readme",
    "dependencies": {},
    "devDependencies": {
        "@cypress/code-coverage": "^1.11.0",
        "concurrently": "^5.1.0",
        "cypress": "^3.8.3",
        "istanbul-lib-coverage": "^3.0.0"
    }
}
